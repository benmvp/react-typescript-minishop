{
  "name": "react-typescript-minishop",
  "version": "0.1.0",
  "private": true,
  "license": "GPL-3.0-only",
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "scripts": {
    "setup": "node scripts/setup.js",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "npm run test -- --watchAll=false",
    "lint": "eslint src --ext .js,.ts,.tsx",
    "type": "tsc --project . --noEmit",
    "validate": "npm-run-all --parallel lint type build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.43",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "typescript": "^3.7.5",
    "url-lib": "^3.0.3"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5"
  }
